{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "clean": "rm -rf node_modules/ && npm install && npm run clean:watchman",
    "clean:watchman": "watchman watch-del-all && watchman shutdown-server",
    "lint": "eslint 'src/'; exit 0",
    "lint:fix": "eslint --fix 'src/'; exit 0",
    "format": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "axios": "^0.19.2",
    "expo": "^38.0.0",
    "expo-camera": "~8.3.1",
    "expo-file-system": "~9.0.1",
    "expo-permissions": "~9.0.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.2",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-reanimated": "~1.9.0",
    "react-native-screens": "~2.9.0",
    "react-native-web": "~0.11.7",
    "react-navigation": "^3.11.0",
    "react-redux": "^5.1.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-preset-expo": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-react-native": "^3.6.0",
    "husky": "^2.2.0",
    "jest-expo": "^38.0.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "react-test-renderer": "16.3.1",
    "reactotron-react-native": "^3.2.1"
  },
  "private": true
}
