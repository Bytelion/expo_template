{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "clean": "rm -rf node_modules/ && npm install && npm run clean:watchman",
    "clean:watchman": "watchman watch-del-all && watchman shutdown-server",
    "lint": "eslint 'src/'; exit 0",
    "lint:fix": "eslint --fix 'src/'; exit 0",
    "format": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "axios": "^0.21.1",
    "expo": "^40.0.0",
    "expo-app-loading": "^1.0.1",
    "expo-camera": "~9.1.0",
    "expo-file-system": "~9.3.0",
    "expo-notifications": "~0.8.2",
    "expo-permissions": "~10.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.0.tar.gz",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.0",
    "react-native-web": "^0.14.9",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn jest"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "projects": [
      {
        "preset": "jest-expo/ios",
        "transformIgnorePatterns": [
          "node_modules/(?!static-container)/"
        ]
      },
      {
        "preset": "jest-expo/android",
        "transformIgnorePatterns": [
          "node_modules/(?!static-container)/"
        ]
      }
    ],
    "testEnvironment": "jsdom",
    "timers": "fake",
    "testTimeout": 5000,
    "verbose": true
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "^8.3.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "husky": "^4.3.6",
    "jest-expo": "^40.0.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^17.0.1",
    "reactotron-react-native": "^5.0.0"
  },
  "private": true
}
